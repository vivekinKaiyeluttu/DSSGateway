<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity WorkFlowUser.
    -->
    <changeSet id="20200105022006-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="work_flow_id"
                                 baseTableName="work_flow_user"
                                 constraintName="fk_work_flow_user_work_flow_id"
